extern fn printInt(i64): i64;

fn fac(n: i64): i64 {
    let result: i64 = 1;
    let i: i64 = 2;
    while (i < n+1) {
      result *= i;
      i += 1;
    }
    return result;
}

fn main(): bool {
  let result: i64 = fac(6);
  printInt(result);
  if (result == 720) {
    return true;
  }
  return false;
}
